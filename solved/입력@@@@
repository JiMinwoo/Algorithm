h = int(input())
house = [list(map(int,input().split())) for _ in range(h)]
answer = 99999999999

def dfs(cost,beforeColor,houseNum):

    global answer

    if houseNum == h:
        if answer >= cost:
            answer = cost
        return answer

    for i in range(3):
        if beforeColor != i:
            dfs(cost+house[houseNum][i],i,houseNum + 1)

dfs(0,4,0)
print(answer)